{
  "manifest_version": 2,
  "name": "Positive Quotes",
  "description": "This extension will give you some random positive quotes.",
  "version": "1.0",
  "icons": {
    "128": "./images/icon128.png",
    "48": "./images/icon48.png",
    "16": "./images/icon16.png"
  },
  "browser_action": {
    "default_icon": "./images/icon16.png",
    "default_popup": "popup.html",
    "default_title": "Positive Quotes"
  }
}